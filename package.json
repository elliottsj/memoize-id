  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-jest": "^6.0.1",
    "babel-preset-es2015": "^6.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.1",
    "eslint-plugin-react": "^3.16.1",
